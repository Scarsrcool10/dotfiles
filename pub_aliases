# Best Aliases
alias gst='git status'
alias b='bundle exec'
alias bcap='bundle exec cap'
alias brake='bundle exec rake'
alias brails='bundle exec rails'
alias bil='bundle install --path vendor/bundle'
alias up='git pull'
alias glo='git log'
alias autocop='bundle exec rake rubocop:lint_unpushed:auto_correct'
alias ms='bundle exec rake mysql'
alias msa='bundle exec rake mysql\[-A\]'
alias tdsd='td && sd'
alias dot=' cd $HOME/dev/dotfiles'

# SHELL
alias ll='ls -lah'
alias reload='exec $SHELL -l'